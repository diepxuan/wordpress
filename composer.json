    "description": "The easiest way to install the WordPress use Composer",
    "license": [
        "OSL-3.0",
        "AFL-3.0",
        "GPL-2.0+"
    ],
    "version": "0.0.1",
    "keywords": [
        "wordpress",
        "blog",
        "cms"
    ],
    "type": "composer-plugin",
    "authors": [{
        "name": "Tran Ngoc Duc",
        "email": "caothu91@gmail.com"
    }, {
        "name": "Tran Ngoc Duc",
        "email": "ductn@diepxuan.com"
    }],
    "require": {
        "php": ">=5.2.4",
        "composer-plugin-api": "~1.0",
        "composer/installers": "~1.0",
        "diepxuan/wordpress-core": "~4.8"
    },
    "require-dev": {},
    "repositories": [{
        "type": "package",
        "package": {
            "name": "diepxuan/wordpress-core",
            "type": "wordpress-core",
            "version": "4.8.2",
