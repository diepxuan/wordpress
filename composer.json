{
    "name": "diepxuan/wordpress",
    "description": "The easiest way to install the WordPress use Composer",
    "license": "GPL-2.0+",
    "version": "0.0.1",
    "keywords": [
        "wordpress",
        "blog",
        "cms"
    ],
    "authors": [{
        "name": "Tran Ngoc Duc",
        "email": "caothu91@gmail.com"
    }],
    "require": {
        "php": ">=5.2.4",
        "composer-plugin-api": "~1.0",
        "composer/installers": "~1.0",
        "diepxuan/wordpress-core": "4.8.2"
    },
    "require-dev": {},
    "repositories": [{
        "type": "package",
        "package": {
            "name": "diepxuan/wordpress-core",
            "type": "wordpress-core",
            "version": "4.8.2",
            "source": {
                "url": "git@github.com:WordPress/WordPress.git",
                "type": "git",
                "reference": "4.8.2"
            }
        }
    }],
    "extra": {
        "installer-paths": {
            "app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
            "app/plugins/{$name}/": ["type:wordpress-plugin"],
            "app/themes/{$name}/": ["type:wordpress-theme"]
        }
    },
    "config": {
        "use-include-path": true
    },
    "minimum-stability": "alpha",
    "prefer-stable": true
}
